
@{
    ViewBag.Title = "Selenuim";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Selenuim</h2>

<div class="column">
    <br><br><br><br>
    <div class="container page-wrapper">
        <div class="page-inner">
            <div class="row">
                <br><br><br><br><div class="el-wrapper">
                    <div class="box-up">
                        <img class="img" src="~/images/selenium.jpg" alt="">
